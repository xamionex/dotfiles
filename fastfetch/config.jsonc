{
  "$schema": "https:github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "$(\"$HOME\"/.config/fastfetch/get_distro_logo.sh)",
    "type": "kitty",
    "height": 15,
    "padding": {
      "top": 2,
      "left": 2
    }
  },
  "display": {
    "separator": " >> ",
    "key": {
        "width": 33,       // Aligns keys to this width
        "type": "both",   // string, icon, both, or none
    },
    "percent": {
        "type": 3         // 1=number, 2=bar, 3=both, 9=colored number
    }
  },
  "modules": [
    {
      "type": "command",
      "key": " ",
      "format": "{result}",
      "text": "\"$HOME\"/.config/fastfetch/line_helper.sh header"
    },
    "break",
    {
      "type": "command",
      "key": " ",
      "format": "üî• {result} üî•",
      "text": "shuf -n1 \"$HOME\"/.config/fastfetch/quotes"
    },
    "break",
    {
      "type": "title",
      "key": "User",
      "keyIcon": "Ôàõ",
      "format": "{1}",
      "keyColor": "31"
    },
    {
      "type": "title",
      "key": "Hostname",
      "format": "{2}",
      "keyColor": "31"
    },
    {
      "type": "os",
      "keyColor": "31"
    },
    {
      "type": "kernel",
      "keyColor": "31"
    },
    {
      "type": "bootmgr",
      "keyIcon": "ÓØê",
      "keyColor": "31"
    },
    {
      "type": "packages",
      "keyColor": "31"
    },
    "break",
    {
      "type": "wm",
      "keyColor": "32"
    },
    {
      "type": "terminal",
      "keyIcon": "ÔÑ†",
      "keyColor": "32"
    },
    // slow for some reason
    //{
    //  "type": "terminalfont",
    //  "key": "‚ï†‚ïæ ÔÄ± Terminal Font",
    //  "keyColor": "32"
    //},
    {
      "type": "icons",
      "keyColor": "32"
    },
    {
      "type": "cursor",
      "keyIcon": "Û∞≥Ω",
      "keyColor": "32"
    },
    "break",
    {
      "type": "command",
      "keyIcon": "Û±¶ü",
      "key": "OS Age",
      "keyColor": "33",
